  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --hot",
    "production": "./node_modules/.bin/webpack -p --optimize-minimize",
    "bundle": "./node_modules/.bin/webpack --hide-modules --colors --mode=none",
    "watch": "./node_modules/.bin/webpack --hide-modules --colors --mode=none --watch",
    "storybook": "./node_modules/.bin/start-storybook -p 9001",
    "build-storybook": "./node_modules/.bin/build-storybook",
    "lint": "eslint ./src/",
    "autoprefixer": "./node_modules/.bin/postcss ./src/css/*.css -r postcss.config.js",
    "format": "clang-format --version; find src test | grep '\\.js$\\|\\.ts$' | xargs clang-format --style=file -i",
    "test-detail": "npm run bundle && npm run lint && wct -p --npm",
    "test": "wct -l chrome --npm",
    "test2": "./node_modules/.bin/mocha test/*.js --recursive -w -r jsdom-global/register",
    "test3": "./node_modules/.bin/mocha test/*.js -r @babel/register -r babel-polyfill -r esm -r jsdom-global/register -r jsdom -r jsdom-global -r custom-elements-es5-adapter -r @webcomponents/webcomponentsjs"
  },
